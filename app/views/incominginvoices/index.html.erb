


<div class="row">
  <div class="col-sm-6">
    <h1>Incominginvoices</h1>
  </div>

  <div class="col-sm-6 text-right">
  <%= link_to new_incominginvoice_path, class: 'btn btn-primary' do %>
    Add New Incominginvoice
  <% end %>
  </div>
</div>

<div class="table-responsive">
  <table id = "example" class="table table-striped table-bordered table-hover">
    <thead>
      <tr>
    

            <th>Number</th>
            <th>Date</th>
            <th>Supplier</th>
            <th>Amount</th>
            
          <th></th>
        
      </tr>
    </thead>

    <tbody>
      <% @incominginvoices.each do |incominginvoice| %>
        <%= content_tag :tr, id: dom_id(incominginvoice), class: dom_class(incominginvoice) do %>
          

                      <td><%= incominginvoice.number %></td>
                      <td><%= incominginvoice.date %></td>
                      <td><%= incominginvoice.supplier %></td>
                      <td><%= incominginvoice.amount %></td>
          
          
            <td><%= link_to 'Show', incominginvoice %></td>
          
        <% end %>
      <% end %>
    </tbody>
  </table>
</div>
