<div class="page-header">
  <%= link_to suppliers_path, class: 'btn btn-default' do %>
    All Suppliers
  <% end %>
  <%= link_to edit_supplier_path(@supplier), class: 'btn btn-primary' do %>
    Edit
  <% end %>
  <h1>Show supplier</h1>
</div>

<dl class="dl-horizontal">
  <dt>Name:</dt>
  <dd><%= @supplier.name %></dd>

  <dt>Email:</dt>
  <dd><%= @supplier.email %></dd>

  <dt>Tel:</dt>
  <dd><%= @supplier.tel %></dd>

  <dt>Address:</dt>
  <dd><%= @supplier.address %></dd>

</dl>
